________________________________________________________________>smallest Prime Factor<___________________________________________________

int N = 1000000;
int prime[10000001];
void createSieve(){
  for(int i=0;i<=N;i++) prime[i]=i;
  for(int i=2;i*i<=N;i++){
    if(prime[i]==i){
      for(int j=i*i;j<=N;j+=i){
        if(prime[j]==j){
          prime[j]=i;
        }
      }
    }
  }
}

------------------------------------------->the sieve of erathosthenes<--------------------------------------------------------------------

 vector<bool>prime(1e6,true);
  prime[0]=prime[1]=false;
  int n; cin>>n;
  for(int i=2;i*i<=n;i++){
    if(prime[i]==true){
      for(int j=i*i;j<=n;j+=i){
        prime[j]=false;
      }
    }
  }
  for(int i=0;i<=n;i++){
    if(prime[i]) cout<<i<<" ";
  }
  
  TIME COMPLEXITY - O(Nlog(log(N))
  ------------------------------------------------------->optimised sieve<-----------------------------------------------------------------
  
